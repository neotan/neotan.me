    "dotenv": "^16.0.0",
    "esbuild": "^0.14.36",
    "framer-motion": "^6.3.3",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "mdx-bundler": "^9.0.0",
