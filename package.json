{
  "name": "neotan.me",
  "version": "0.3.5",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:mdx": "node ./scripts/compile-mdx.js ./content",
    "clean": "rimraf ./node_modules ./package-lock.json ./yarn.lock ./.temp ./.next",
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "dev:mdx": "chokidar ./tailwind.config.js ./content/**/*.mdx ./utils/mdx.js -c \"npm run build:mdx\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\" && npm run lint",
    "lint": "next lint --fix",
    "prebuild": "npm run build:mdx",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@cloudinary/react": "^1.5.0",
    "@cloudinary/url-gen": "^1.8.6",
    "@tailwindcss/typography": "^0.5.7",
    "clsx": "^1.2.1",
    "daisyui": "^2.31.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.5.2",
    "fuse.js": "^6.6.2",
    "next": "^12.3.1",
    "next-images": "^1.8.4",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-daisyui": "^2.4.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-use": "^17.4.0",
    "tailwind-merge": "^1.6.1",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@types/node": "^16.11.64",
    "@types/ramda": "^0.28.15",
    "@types/react": "^18.0.21",
    "autoprefixer": "^10.4.12",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esbuild": "~0.14.54",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "json-stringify-safe": "^5.0.1",
    "mdx-bundler": "^9.0.1",
    "node-readfiles": "~0.2.0",
    "postcss": "^8.4.17",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "reading-time": "^1.5.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-images": "^3.1.0",
    "remark-prism": "^1.3.6",
    "remark-slug": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
